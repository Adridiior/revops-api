{
  "scripts": {
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "prisma generate && tsc",
    "start": "npx prisma db push && if [ \"$RUN_SEED\" = \"true\" ]; then npm run seed:prod; fi && node dist/src/index.js",
    "migrate": "prisma migrate dev",
    "seed": "ts-node prisma/seed.ts",
    "seed:prod": "node dist/prisma/seed.js",
    "studio": "prisma studio"
  },
  "dependencies": {
    "@prisma/client": "6.19.2",
    "cors": "^2.8.6",
    "dotenv": "^17.2.4",
    "express": "^5.2.1",
    "pg": "^8.18.0",
    "prisma": "6.19.2",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^25.2.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  }
}
